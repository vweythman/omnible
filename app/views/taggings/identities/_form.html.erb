<% remote ||= false %>
<%= form_for @identity, remote: remote do |f| %>

	<fieldset>
		<%= form_field f.label(:nature), f.text_field(:nature, :placeholder => "Describe the Type of Identity") %>
		<%= form_field f.label(:name), f.text_field(:name, :placeholder => "Name the Identity") %>
	</fieldset>

	<%= f.submit %>

<% end %>
