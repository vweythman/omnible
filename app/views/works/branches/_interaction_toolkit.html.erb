<% if branch.editable? current_user %>
	<div id="branching-form"></div>

	<div class="toolkit generation">
		<%= link_to "New Branch", branch_bubble_path(branch), class:"icon icon-leaf tool-link" %>
		<%= link_to( "Graft to Existing Branch", graft_branch_path(branch), remote: true , class:"icon icon-leaf tool-link") if branch.graftable? %>
	</div>
<% end %>
