<%= form_for(@article) do |f| %>

	<fieldset class="primary">
		<%= render @article.primary_fields, f: f %>

		<%= f.fields_for :note do |b| %>
			<%= text_panel b, :content, :major, "Write Article" %>
		<% end %>
	</fieldset>
	
	<%= render @article.tag_fields, f: f %>

	<%= submit_panel f %>
<% end %>
