<div class='nested-fields'>
	<fieldset>
		<%= form_field f.label(:item_id, "Item"), f.collection_select(:item_id, @items.possible_possessions, :id, :heading, {}, :class => 'selectables') %>
		<%= radios_by_string(f, :nature, Possession.types, f.object.nature) %>
	</fieldset>

	<p class="remove-cell"><%= link_to_remove_association "- Remove Item", f %></p>
</div>
